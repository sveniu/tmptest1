{
  "family": "mypi-preview-frontend-ns",
  "executionRoleArn": "arn:aws:iam::498317514113:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "container1",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mypi-preview-frontend-ns",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::498317514113:role/app-mypi",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}
